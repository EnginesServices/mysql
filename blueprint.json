{
  "title": "mysql server",
  "description": "mysql",
  "memory_usage": 456,
  "licenses": [
  ],
  "binding_anchor": {
    "variables": {
      "flavor": "mysql2",
      "host": "^^identifier^^.^^universe.host^^",
      "name": "^^identifier^^",
      "username": "^^identifier^^_user",
      "password": "^^random(10)^^",
      "collation": "utf8_general_ci",
      "full_access": "0",
      "backup_support": true
    }
  },
  "bindings": [
    {
      "descriptor": {
        "repository": "https://github.com/MarkRatjens/logrotate.git",
        "branch": "spaces"
      }
    }
  ],
  "stages": [
    {
      "starter": {
        "image": "engines/servicebase",
        "tag": "current"
      },
      "os_packages": [
        {
          "name": "mysql-server"
        },
        {
          "name": "mysql-client"
        }
      ]
    }
  ],
  "environment": {
    "variables": [
      {
        "name": "rails_flavor",
        "value": "mysql2"
      },
      {
        "name": "backup_support",
        "value": "true"
      },
      {
        "name": "db_flavor",
        "value": "mysql2"
      }
    ]
  }
}
